{
  "name": "react-monaco-editor-component",
  "version": "0.0.6",
  "description": "基于 monaco-edito 实现一套 web-ide 相关组件",
  "author": "yunliang-ding",
  "homepage": "https://yunliang-ding.github.io/react-monaco-editor-component",
  "keywords": [
    "web-ide",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunliang-ding/react-monaco-editor-component"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "lodash": "4.17.21",
    "monaco-editor": "0.33.0",
    "react-dom": "17.0.2",
    "react-core-form-designer": "0.0.6",
    "react-contexify": "5.0.0",
    "@octokit/core": "4.0.5"
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "react-core-form": "0.0.15",
    "@types/lodash": "4.14.168",
    "@umijs/test": "^3.0.5",
    "@types/ali-oss": "6.16.3",
    "@babel/runtime": "7.16.0",
    "babel-plugin-import": "1.13.3",
    "dumi": "^1.0.14",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.26.1",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "less": "2.7.3",
    "lint-staged": "^10.0.7",
    "monaco-editor-webpack-plugin": "7.0.1",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0",
    "@types/react": "17.0.2",
    "react-split-pane": "0.1.92"
  },
  "license": "MIT"
}